{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "types": [ "vue"]
  },
  "include": ["env.d.ts", "src/**/*", ".vitepress/**/*"]
}